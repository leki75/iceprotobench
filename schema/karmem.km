karmem karmem @packed(true) @golang.package(`schema`);  
  
struct KarmemTrade table {
    Id         uint64;
    Timestamp  uint64;
    Price      uint64;
    Volume     uint32;
    Conditions [4]byte;
    Symbol     [11]byte;
    Exchange   byte;
    Tape       byte;
    ReceivedAt uint64;
}

struct KarmemTradeData inline {
    Data KarmemTrade;
}

struct KarmemTrades table {
    Trades [<2000]KarmemTradeData;
}

